  "request_process": {
    "eligibility": "All persons entitled to greatest possible information regarding government affairs",
    "identification_required": "Must adequately describe records sought",
    "response_timeframe": "As soon as practicable and without delay (generally 10 business days for simple requests)",
    "appeal_process": "Direct court action for mandamus (no administrative appeals)",
    "fees": {
      "inspection": "Free (no charges for access)",
      "copying": "Actual, necessary, and direct cost of reproduction",
      "location_fees": "Only if actual cost exceeds $50 (with advance written estimate)",
      "review_time": "Cannot charge for time reviewing records to determine access rights",
      "standard_copying": "Typically $0.25 per page for photocopies"
    }
  },
  "key_features": {
    "presumption_of_access": "Construed in every instance with presumption of complete public access",
    "greatest_possible_information": "Policy declares all persons entitled to greatest possible information",
    "essential_government_function": "Providing information declared essential function of representative government",
    "denial_contrary_to_public_interest": "Denial generally contrary to public interest, only exceptional cases may deny",
    "attorney_fees": "Reasonable attorney fees available to substantially prevailing requesters",
    "minimum_damages": "$100 minimum damages for arbitrary and capricious denials",
    "record_destruction_prohibition": "Cannot destroy records after request received until 60 days after denial or completion of proceedings",
    "electronic_records_creation": "Must create electronic records if technically feasible and not unduly burdensome"
  }
}